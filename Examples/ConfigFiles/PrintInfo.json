{
    "processor-toolbox":
    {
        "processors":
        [
	        { "type": "egg-processor",   "name": "egg" },
	        { "type": "terminal-writer", "name": "writer" }
        ],
        
        "connections":
        [
	        {
	            "signal": "egg:header",
	            "slot": "writer:header"
	        },

	        {
	            "signal": "egg:summary",
	            "slot": "writer:summary"
	        }
        ],
        
        "run-queue":
        [
            "egg"
        ]
    },
    

    "egg":
    {
        "filename": "/path/to/file.egg",
        "egg-reader": "egg3",
        "slice-size": 16384,
        "number-of-slices": 500
    },
    
}