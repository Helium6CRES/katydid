{
    "processor-toolbox":
    {
        "processors":
        [
            { "type": "egg-processor",          "name": "egg" },
            { "type": "fractional-fft",         "name": "frac" },
            { "type": "forward-fftw",           "name": "fft" },
            { "type": "convert-to-power",       "name": "to-ps" },
            { "type": "root-spectrogram-writer",       "name": "rootw"}
        ],
        
        "connections":
        [
            {
                "signal": "egg:header",
                "slot": "fft:header"
            },

            {
                "signal": "egg:ts",
                "slot": "frac:ts-chirp"
            },

            {
                "signal": "frac:ts",
                "slot": "fft:ts-fftw"
            },

            {
                "signal": "fft:fft",
                "slot": "to-ps:fs-fftw-to-psd"
            },

            {
                "signal": "to-ps:psd",
                "slot": "rootw:psd"
            },

            {
                "signal": "egg:egg-done",
                "slot": "rootw:write-file"
            }
        ],
        
        "run-queue":
        [
            "egg"
        ]
    },
        
    "egg":
    {
        "filename": "locust_faketrack.egg",
        "egg-reader": "egg3",
        "number-of-slices": 0,
        "slice-size": 4096

    },

    "frac":
    {
        "slope": 0.0e9
    },

    "fft":
    {
        "transform-flag": "ESTIMATE"
    },

    "rootw":
    {
        "output-file": "spectrogram-frfft.root",
        "min-time": 0.0,
        "max-time": 0.03,
        "min-freq": 15.0e6,
        "max-freq": 30.0e6,
        "file-flag": "recreate"
    }
}
