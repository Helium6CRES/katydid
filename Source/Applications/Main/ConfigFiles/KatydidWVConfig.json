{
    "katydid":
    {
        "processor-toolbox":
        {
            "processor":
            {
                "type": "egg-processor",
                "name": "egg",
                "is-top-level": true
            },
            "processor":
            {
                "type": "wigner-ville",
                "name": "wv",
                "is-top-level": true
            },
            "processor":
            {
                "type": "hough-transform",
                "name": "hough",
                "is-top-level": true
            },
            "processor":
            {
                "type": "basic-root-writer",
                "name": "writer",
                "is-top-level": true
            },
            
            "connection":
            {
                "signal-processor": "egg",
                "signal-name": "header",
                "slot-processor": "wv",
                "slot-name": "header"
            },
            "connection":
            {
                "signal-processor": "egg",
                "signal-name": "event",
                "slot-processor": "wv",
                "slot-name": "event",
                "group-order": 0
            },
            "connection":
            {
                "signal-processor": "egg",
                "signal-name": "event",
                "slot-processor": "hough",
                "slot-name": "event",
                "group-order": 1
            },
            "connection":
            {
                "signal-processor": "wv",
                "signal-name": "wigner-ville",
                "slot-processor": "writer",
                "slot-name": "write-data"
            },
            "connection":
            {
                "signal-processor": "hough",
                "signal-name": "hough-transform",
                "slot-processor": "writer",
                "slot-name": "write-data"
            },
            
            "run-queue":
            {
                "processor": "egg"
            }
        },
        

        "egg":
        {
            "filename": "/path/to/file.egg",
            "number-of-events": 1,
            "time-series-size": 0,
            "time-series-type": "fftw",
            "output-data-name": "time-series"
        },
        
        "wv":
        {
            "wv-pair": "0, 1",
            "input-data-name": "time-series",
            "output-data-name": "wigner-ville",
            "complex-fftw":
            {
                "transform-flag": "ESTIMATE"
            },
            "sliding-window-fftw":
            {
                "transform-flag": "ESTIMATE",
                "overlap-size": 0,
                "window-function-type": "rectangular",
                "window-function":
                {
                    "size": 512
                }
            }
        },
        
        "hough":
        {
            "n-theta-points": 100,
            "n-r-points": 100,
            "input-data-name": "wigner-ville"
        },
        
        "writer":
        {
            "output-file": "/path/to/file.root",
            "file-flag": "recreate"
        }
    }
}