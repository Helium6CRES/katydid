# KatydidConfig.cmake

if (@PROJECT_NAME@_FOUND)
    return()
endif (@PROJECT_NAME@_FOUND)
set (@PROJECT_NAME@_FOUND TRUE)

set (ROOT_FOUND @ROOT_FOUND@)
if (ROOT_FOUND)
    include_directories(@ROOT_INCLUDES@)
endif (ROOT_FOUND)

# Make customizations below

set (@PROJECT_NAME@_DEPENDS_LIBRARIES @LIBS@)

set (@PROJECT_NAME@_VERSION_MAJOR @Katydid_VERSION_MAJOR@)
set (@PROJECT_NAME@_VERSION_MINOR @Katydid_VERSION_MINOR@)
set (@PROJECT_NAME@_REVISION @Katydid_REVISION@)
set (@PROJECT_NAME@_VERSION @Katydid_VERSION@)
set (@PROJECT_NAME@_FULL_VERSION @Katydid_FULL_VERSION@)
set (@PROJECT_NAME@_PACKAGE_NAME @Katydid_PACKAGE_NAME@)
set (@PROJECT_NAME@_PACKAGE_STRING @Katydid_PACKAGE_STRING@)

set(@PROJECT_NAME@_INCLUDE_DIRS @INCLUDE_INSTALL_DIR@ @Monarch_INCLUDE_DIR@)
set(@PROJECT_NAME@_LIBRARY_DIRS @LIB_INSTALL_DIR@ @Monarch_LIBRARY_DIR@)
set(@PROJECT_NAME@_LIBRARIES @Katydid_LIBRARIES@ @Monarch_LIBRARIES@)